{
  "name": "sdfx",
  "version": "0.8.8",
  "main": "dist_electron/electron/main/index.js",
  "description": "The Studio Grade UI for AI Synthesis",
  "author": "SDFX",
  "license": "GNU AFFERO",
  "private": true,
  "scripts": {
    "start": "npm run dev:app",
    "dev:app": "vite --config ./vite.config.app.ts --host",
    "dev:web": "vite --config ./vite.config.web.ts",
    "build:front:app": "vue-tsc --noEmit && vite build --config ./vite.config.app.ts",
    "build:front:web": "vue-tsc --noEmit && vite build --config ./vite.config.web.ts",
    "build:electron": "electron-builder",
    "build": "npm run build:front:app && npm run build:electron",
    "exec": "start dist_electron/win-unpacked/SDFX.exe",
    "be": "npm run build && npm run exec",
    "preview": "vite preview"
  },
  "engines": {
    "node": "~18.9.1 || ~18.10.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
